<h2>New Lunches</h2>
<%= form_for(@lunch) do |f| %>
	<%= f.error_messages %>
	<p>
		<%= f.label :food %><br />
		<%= f.text_field :food %>
	</p>

	<p>
		<%= f.label :drink %><br />
		<%= f.text_field :drink %>
	</p>

	<p>
		<%= f.label :cost %><br />
		<%= f.text_field :cost %>
	</p>

	<%= f.submit %>
<% end %>
